e1.9:
	g++ -g -Wall -o e1.9 e1.9.cc
fstream:
	g++ -g -Wall -o fstream fstream.cc
t1.1:
	g++ -g -Wall -o t1.1 t1.1.cc
t1.2:
	g++ -g -Wall -o t1.2 t1.2.cc
t1.4.1:
	g++ -g -Wall -o t1.4.1 t1.4.1.cc
vector:
